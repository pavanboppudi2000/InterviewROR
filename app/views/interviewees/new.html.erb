<h3> Adding a New Interviewee</h3>
<%= form_for :interviewee , html: { multipart: true } , url: interviewees_path(@interviewee) do |f| %>
   <%if @interviewee.errors.any? %> 
       <% @interviewee.errors.full_messages.each do |msg| %>
          <%= msg %><br>
        <% end %>
    <% end %> 
    <p>
        <%= f.label :Email %><br>
        <%= f.text_field :email %>
    </p>
 
    <p>
        <%= f.label :Name %><br>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :College %><br>
        <%= f.text_field :clg %>
    </p>
    <p>
        <%= f.label :Cgpa %><br>
        <%= f.text_field :cgpa %>
    </p>


    

    <p>
          <%= f.submit :create%>
    </p>
<% end %>