<h3> Editing Schedule<h3>
<%= form_for :schedule , url: schedule_path(@schedule),method: :patch do |f| %>
    <p>
        <%= f.label :Interviewer_Email %><br>
        <%= f.text_field :email1 %>
    </p>
 
    <p>
        <%= f.label :Interviewee_Email %><br>
        <%= f.text_field :email2 %>
    </p>
    <p>
        <%= f.label :start_Time %><br>
        <%= f.datetime_field :st %>
    </p>
    <p>
        <%= f.label :end_Time %><br>
        <%= f.datetime_field :end%>
    </p>


    <p>  
          <%= f.submit :Edit%>
    </p>
   
<% end %>