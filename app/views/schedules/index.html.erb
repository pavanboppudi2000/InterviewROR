<h2><%= link_to "New Interview" , new_schedule_path%></h2>
<% @schedule.each do |s|%>
    <h3>Interviewer  :<%= s.email1 %><br>
    Interviewee :<%= s.email2 %> </h3>
    Start Time :<%= s.st %><br>
    End Time :<%= s.end %><br><br>
    <h3><%= link_to "View" , schedule_path(s)%>
    <%= link_to "Edit" , edit_schedule_path(s)%>
    <%= link_to "Delete" , schedule_path(s),method: :delete, data: {confirm: 'You are to delete a task?' }%></h3>

<% end %>