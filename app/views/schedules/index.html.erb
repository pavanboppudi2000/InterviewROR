<h2><%= link_to "New Interview" , new_schedule_path%></h2>
<% @schedule.each do |interviewee|%>
    <h3>Interviewer  :<%= interviewee.email1 %><br>
    Interviewee :<%= interviewee.email2 %> </h3>
    Start Time :<%= interviewee.st %><br>
    End Time :<%= interviewee.end %><br><br>
    <h3><%= link_to "View" , schedule_path(interviewee)%>
    <%= link_to "Edit" , edit_schedule_path(interviewee)%>
    <%= link_to "Delete" , schedule_path(interviewee),method: :delete, data: {confirm: 'You are to delete a task?' }%></h3>

<% end %>